eleitores = int(input("quantos eleitores tem na sua cidade?"))
votos_brancos = int(input("quantos votos brancos tiveram?"))
nulos = int(input("quantos votos nulos tiveram?"))
v치lidos = int(input("quantos votos v치lidos tiveram?"))

resultado1 = (votos_brancos/eleitores) *100
resultado2 = (nulos / eleitores) * 100
resultado3 = (v치lidos / eleitores) * 100

print(f"Percentual de votos brancos: {resultado1}%")
print(f"Percentual de votos nulos: {resultado2}%")
print(f"Percentual de votos v치lidos: {resultado3}%")
